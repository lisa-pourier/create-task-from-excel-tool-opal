{
  "name": "@zaiusinc/app-forms-schema",
  "version": "2.0.0",
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "files": [
    "dist/",
    "src/"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=22.0"
  },
  "scripts": {
    "build": "yarn update-schema && tsc && cp ./src/schema/Form.schema.json ./dist/",
    "lint": "npx eslint src",
    "prettier": "prettier --write 'src/**/*.ts'",
    "test": "jest --coverage",
    "update-schema": "ts-json-schema-generator -p 'src/**/*.ts' -t Form --no-type-check --no-top-ref -o ./src/schema/Form.schema.json"
  },
  "dependencies": {
    "ajv": "^8.17.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@types/jest": "^29.5.14",
    "@zaiusinc/eslint-config-presets": "^2.0.0-beta.5",
    "eslint": "^9.21.0",
    "eslint-config-love": "^119.0.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.37.4",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.6",
    "ts-json-schema-generator": "^2.3.0",
    "typedoc": "^0.27.9",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  }
}
